{
  "name": "in-and-out",
  "version": "1.3.254",
  "description": "In&Out web-app",
  "main": "./src/game.js",
  "private": true,
  "scripts": {
    "server": "env TZ='Europe/Amsterdam' node ./web-app-deploy/server.js",
    "start": "NODE_ENV=development node_modules/nodemon/bin/nodemon.js ./web-app-deploy/server.js --watch web-app-deploy",
    "build": "webpack --config webpack/webpack.config.build.js --env.patch",
    "backup": "node ./backup/backup.js",
    "clone": "node ./backup/clone.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massicattaneo/in-and-out.git"
  },
  "author": "Massimiliano Cattaneo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/massicattaneo/in-and-out/issues"
  },
  "nodemonConfig": {
    "ignore": [
      "**/web-app-deploy/backup-db/**"
    ]
  },
  "homepage": "https://github.com/massicattaneo/in-and-out#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.18.2",
    "box-node-sdk": "^1.22.0",
    "compression": "^1.7.1",
    "connect-mongo": "^2.0.0",
    "express": "^4.16.2",
    "express-brute": "^1.0.1",
    "express-brute-mongo": "^1.0.0",
    "express-fileupload": "^0.4.0",
    "express-session": "^1.15.6",
    "googleapis": "^23.0.0",
    "mongodb": "^2.2.33",
    "mongodb-backup": "^1.6.9",
    "node-schedule": "^2.1.1",
    "nodemailer": "^4.4.1",
    "nodeJs-zip": "^1.0.4",
    "nodemon": "^2.0.21",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.8.3",
    "pdftoimage": "^1.0.0",
    "qrcode": "^1.2.0",
    "sharp": "^0.28.3",
    "sitemap": "^1.13.0",
    "spider-detector": "^1.0.18",
    "stripe": "^6.36.0",
    "striptags": "^3.1.1",
    "ws": "^5.1.1",
    "xlsx": "^0.14.5"
  }
